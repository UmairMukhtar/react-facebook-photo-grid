    "infinite-react-carousel": "^1.2.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "babel-loader": "^8.0.6",
    "prop-types": "^15.4.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
